                "type": "text/javascript",
                "exec": [
                  "try {",
                  "  const body = res.getBody();   ",
                  "  const token = body && body.token;",
                  "",
                  "  if (token && typeof bru !== 'undefined') {",
                  "",
                  "    bru.setEnvVar('jwtToken', token);",
                  "    bru.setVar('jwtToken', token);",
                  "",
                  "    console.log(\"Token salvo no ambiente:\", token);",
                  "  } else {",
                  "    console.log(\"Token não encontrado no body da resposta\");",
