              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code é 200', function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test('Resposta contém dados da rota', function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.success).to.be.true;",
                  "    pm.expect(jsonData.data).to.have.property('route');",
                  "    pm.expect(jsonData.data.route).to.have.property('id');",
                  "    pm.expect(jsonData.data.route).to.have.property('name');",
                  "    pm.expect(jsonData.data.route).to.have.property('collection_points');",
                  "});"
                ]
              }
            }
          ],
